{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "FastFind",
  "version": "0.1.0",
  "identifier": "com.fastfind.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "FastFind",
        "label": "main",
        "width": 1200,
        "height": 760,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false,
    "targets": "all"
  }
}
